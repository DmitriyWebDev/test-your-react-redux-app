{
  "name": "test-your-react-redux-app",
  "version": "1.0.0",
  "main": "./app/index.jsx",
  "repository": "https://github.com/DmitriyWebDev/test-your-react-redux-app.git",
  "author": "DmitriyGavrilov <d-man_real_akkaunt.ru@mail.ru>",
  "license": "MIT",
  "scripts": {
    "start-dev": "concurrently \"npm run api-express\" \"npm run dev\"",
    "api-express": "nodemon server.js",
    "dev": "cross-env HOST=http://localhost:3001 webpack-dev-server --open --config webpack.dev.js",
    "build": "webpack --config webpack.dev.js",
    "prod": "webpack --config webpack.prod.js",
    "test": "node ./scripts/test.js --watchAll",
    "test-latest": "node ./scripts/test.js",
    "prettier": "prettier --write \"./**/*.js?(x)\"",
    "lint": "eslint ./",
    "lint-fix": "eslint --fix ./"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run lint-fix",
      "npm run prettier",
      "git add",
      "jest --bail --findRelatedTests"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@babel/node": "^7.5.0",
