    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "classnames": "^2.2.5",
    "core-js": "^3.1.4",
    "history": "4.7.2",
